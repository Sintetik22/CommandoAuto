local function Tween(time, pos)
    noclip = true
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end

local ccccount = 0

local function killViciousBeeFunc()
for i,v in pairs(workspace.Monsters:GetChildren()) do
    if string.find(v.Name, "Stick Bug") then 
        pcall(function()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
        end)
    end
end
end
for i,v in pairs(workspace.Monsters:GetChildren()) do
    if string.find(v.Name, "Stick Bug") then
            ccccount = ccccount + 1
            if ccccount == 5 then
                cccccount = 0
                Tween(0.5,v.Torso.CFrame - Vector3.new(13, 10, 0))
            else
            Tween(0.5,v.Head.CFrame + Vector3.new(13, 13, 0))
            end
        end
    end


